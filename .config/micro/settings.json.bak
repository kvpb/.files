{
    "pluginchannels": ["https://raw.githubusercontent.com/micro-editor/plugin-channel/master/channel.json"],
    "pluginrepos": [],
    "sucmd": "sudo",
    "autosu": true,
    "encoding": "utf-8",
    "filetype": "unknown",
    "fileformat": "unix",
    "eofnewline": true,
    "mkparents": true,
    "useprimary": true,
    "clipboard": "external",
    "paste": false,
    "mouse": true,
    "xterm": false,

    "rmtrailingws": false,
    "tabstospace": false,
    "keepautoindent": false,
    "autoindent": true,
    "smartpaste": false,

    "ignorecase": true,
    "incsearch": true,
    "splitright": true,
    "splitbottom": true,    
    "scrollmargin": 3,
    "scrollspeed": 2,
    "softwrap": true,
    "keymenu": true,

    "autosave": 0,
    "permbackup": false,
    "backupdir": "",
    "backup": true,
    "saveundo": false,
    "savehistory": true,
    "parsecursor": true,
    "savecursor": true,
    "multiopen": "tab",

    "tabsize": 4,
    "matchbrace": true,
    "indentchar": "‚êâ",
    "diffgutter": true,
    "cursorline": true,
    "colorcolumn": 72,
    "relativeruler": false,
    "ruler": true,
    "hlsearch": true,
    "scrollbarchar": "|",
    "scrollbar": true,
    "basename": false,
    "statusformatl": "$(filename)",
    "statusformatr": "$(bind:ToggleKeyMenu): bindings, $(bind:ToggleHelp): help",
    "infobar": true,
    "tabhighlight": true,
    "tabreverse": true,
    "syntax": true,
    "colorscheme": "atom-dark"
}
