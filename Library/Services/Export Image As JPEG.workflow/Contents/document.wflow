					<string>on run {input, parameters}		tell application "Finder"		if FinderItemSelectionList is not {} then -- 'An empty list (containing no items) is represented by a pair of empty braces: {}.' (https://web.archive.org/web/20180602191246/https://developer.apple.com/library/content/documentation/AppleScript/Conceptual/AppleScriptLangGuide/conceptual/ASLR_lexical_conventions.html)			(* topLevelName assures the uppermost file-level-directory will be selected in case of multiple selections in list or column view. *)			set topLevelName to name of item 1 of FinderItemSelectionList			(* silly set of workarounds to override vanilla AppleScript's lack of permission when playing in the .Trash folders *)			set itemSelectionListPath to POSIX path of file ((item 1 of FinderItemSelectionList) as string)			set itemSelectionListPath to ((characters 1 thru ((offset of topLevelName in itemSelectionListPath) - 1)) in itemSelectionListPath) as string			(* Modify path string to create a unique target folder using date and time *)			set shellScriptOutputOfdate to do shell script "date -j -f \"%a %b %d %T %Z %Y\" \"`date`\" \"+%s\""			set newFolderPathname to itemSelectionListPath &amp; (".tmp" &amp; shellScriptOutputOfdate)			(* more workarounds to allow playing in the .Trash *)			do shell script "mkdir " &amp; quoted form of newFolderPathname			set newFolderPathname to (POSIX file newFolderPathname) as alias			copy FinderItemSelectionList to newFolderPathname		end if	end tell	return newFolderPathnameend run</string>
				</dict>
