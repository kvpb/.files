					<string>ifscopy=${IFS};
IFS=$'\n';
positionalparameternumber=0;
sipsformatOptionspercent=0;

for file in ${@};
do
	if [ "${positionalparameternumber}" = "0" ];
	then
		sipsformatOptionspercent=${file};
	else
		echo "Exporting ${file} as JPEG...";
		filename=${file};
		sips -s format jpeg -s formatOptions ${sipsformatOptionspercent} ${file} --out ${filename%.*}.jpg;
	fi;
	positionalparameternumber=positionalparameternumber+1;
done;

